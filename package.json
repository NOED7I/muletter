{
  "name": "muletter-server",
  "version": "0.8.1",
  "description": "minimalist email list driven by a database-less Web API",
  "keywords": [
    "Node.js",
    "newsletter",
    "API",
    "mailing",
    "list",
    "server",
    "json",
    "http"
  ],
  "author": {
    "name": "Kimi",
    "url": "https://github.com/kimihub"
  },
  "license": "GPL-3.0",
  "homepage": "https://github.com/kimihub/muletter-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/kimihub/muletter-server"
  },
  "engines": {
    "node": ">=10.2.1"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.4",
    "git-pre-commit": "^2.1.4",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  },
  "main": "server/main.js",
  "scripts": {
    "start": "node server/main.js",
    "test": "PORT=8080 ava -s --color -v",
    "test-watch": "PORT=8080 ava -s --watch --color -v",
    "standard": "standard --fix --verbose **/*.js | snazzy",
    "precommit": "npm run standard && npm run test",
    "postinstall": "cd sender; npm install",
    "init": "node --no-warnings scripts/init.js"
  },
  "dependencies": {}
}

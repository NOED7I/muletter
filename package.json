{
  "name": "muletter-server",
  "version": "0.7.4",
  "description": "minimalist email list driven by a database-less Web API",
  "keywords": [
    "Node.js",
    "newsletter",
    "API",
    "mailing",
    "list",
    "server",
    "json",
    "http"
  ],
  "author": {
    "name": "Kimi",
    "url": "https://github.com/kimihub"
  },
  "license": "GPL-3.0",
  "homepage": "https://github.com/kimihub/muletter-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/kimihub/muletter-server"
  },
  "engines": {
    "node": ">=10.2.1"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.4",
    "git-pre-commit": "^2.1.4",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  },
  "main": "server/main.js",
  "scripts": {
    "start": "node server/main.js",
    "test": "NODE_ENV=test PORT=8080 ava **/*.test.js --color -v",
    "test-watch": "NODE_ENV=test PORT=8080 ava **/*.test.js --watch --color -v",
    "standard": "standard --fix --verbose *.js test/*.js | snazzy",
    "precommit": "npm run standard && npm run test",
    "postinstall": "cd sender; npm install",
    "get-status": "node --no-warnings scripts/get-status.js",
    "randomize-keys": "node --no-warnings scripts/randomize-keys.js"
  },
  "dependencies": {}
}

{
  "name": "mulist-server",
  "version": "0.5.0",
  "description": "minimalist email list driven by a database-less Web API",
  "keywords": [
    "Node.js",
    "newsletter",
    "API",
    "mailing",
    "list",
    "server",
    "json",
    "http"
  ],
  "author": {
    "name": "Kimi",
    "url": "https://github.com/kimihub"
  },
  "license": "GPL-3.0",
  "homepage": "https://github.com/kimihub/mulist-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/kimihub/mulist-server"
  },
  "engines": {
    "node": ">=9.4.0"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.4",
    "git-pre-commit": "^2.1.4",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  },
  "main": "server.js",
  "scripts": {
    "test": "NODE_ENV=test PORT=8080 ava test/*.test.js --color -v",
    "test-watch": "NODE_ENV=test PORT=8080 ava test/*.test.js --watch --color -v",
    "standard": "standard --fix --verbose *.js test/*.js | snazzy",
    "precommit": "npm run standard && npm run test"
  },
  "dependencies": {}
